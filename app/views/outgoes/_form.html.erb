<%= form_with model: model do |form| %>
    district <br>
    <%= form.collection_select :district_id, District.all, :id, :name, include_blank: "select something" %> <br>
    year <br>
    <%= form.text_field :year %> <br>
    category <br>
    <%= form.text_field :category %> <br>
    construction name <br>
    <%= form.text_field :name %> <br>
    price <br>
    <%= form.text_field :price %> <br>
    <%= form.submit "submit" %>
<% end %>