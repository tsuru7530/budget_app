<%= form_with model: model do |form| %>
    income <br>
    <%= form.text_field :income_id %> <br>
    year <br>
    <%= form.text_field :year %> <br>
    category <br>
    <%= form.text_field :category %> <br>
    price <br>
    <%= form.text_field :price %> <br>
    memo <br>
    <%= form.text_field :memo %> <br>
    <%= form.submit "submit" %>
<% end %>