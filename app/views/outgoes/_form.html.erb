<%= form_with model: outgo, url: path , local: true do |form| %>
    <%= render partial: "/layouts/error_messages", locals:{model: form.object} %>
    <%= form.hidden_field :income_id, value: @income.id %> <br>
    年度 <br>
    <%= form.text_field :year, class: "form-controll" %> <br>
    金額 <br>
    <%= form.text_field :price, class: "form-controll" %> <br>
    備考 <br>
    <%= form.text_field :memo, class: "form-controll" %> <br><br>
    <%= form.submit "登録", class: "btn btn-primary" %>
<% end %><br>