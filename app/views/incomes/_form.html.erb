<%= form_with model: model do |form| %>
    <%= render partial: "/layouts/error_messages", locals:{model: form.object} %>
    district <br>
    <%= form.collection_select :district_id, District.all, :id, :name, include_blank: "select something" %> <br>
    year <br>
    <%= form.text_field :year %> <br>
    type <br>
    <%= form.select :category, ["initial","correct"] %> <br>
    price <br>
    <%= form.text_field :price %> <br>
    memo <br>
    <%= form.text_field :memo %> <br>
    <%= form.submit "submit" %>
<% end %>